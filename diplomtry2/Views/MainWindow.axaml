							<Image Grid.Column="4" Grid.Row="8" HorizontalAlignment="Left" VerticalAlignment="Bottom" x:Name="FirstWallImageOR" IsVisible="False" Source="avares://diplomtry2/Images/первая стена О---Р.png" Width="200" Height="200" Margin="70,70,0,00"/>
							<Image Grid.Column="4" Grid.Row="8" HorizontalAlignment="Left" VerticalAlignment="Bottom" x:Name="FirstWallImageDR" IsVisible="False" Source="avares://diplomtry2/Images/первая стена --Д-Р.png" Width="200" Height="200" Margin="70,70,0,0"/>
							<TextBlock Grid.Column="0" Grid.Row="3"  HorizontalAlignment="Left" Text="Предельный спектр:" Margin="0,5,10,5"/>
							<ComboBox Foreground="White" Grid.Column="1"  Grid.Row="3" HorizontalAlignment="Left" Margin="0,5" x:Name="comboBox_FirstSpeechLevel" Width="100" SelectedIndex="0" IsVisible="True">
								<ComboBoxItem Content="ПС-20"/>
								<ComboBoxItem Content="ПС-25"/>
								<ComboBoxItem Content="ПС-30"/>	
								<ComboBoxItem Content="ПС-35"/>
								<ComboBoxItem Content="ПС-40"/>
								<ComboBoxItem Content="ПС-45"/>
								<ComboBoxItem Content="ПС-50"/>
								<ComboBoxItem Content="ПС-55"/>
							</ComboBox>
							<TextBlock Grid.Column="0" Grid.Row="0"  HorizontalAlignment="Left" Text="Длина стены, м:" Margin="0,5"/>
							<TextBox Grid.Column="1" Grid.Row="0" HorizontalAlignment="Left" x:Name="FirstWallLenght" Text="2" TextChanging="FirstWallLenght_TextChanged" Margin="0,5" Width="100"/>
							<TextBlock Grid.Column="2" Grid.Row="0"  HorizontalAlignment="Left" Text="Громкость источника звука, Дб:" Margin="-70,5,0,5"/>
							<TextBox Grid.Column="3" Grid.Row="0" HorizontalAlignment="Left" x:Name="FirstWallVolume" Text="50" Margin="-100,5,0,5" Width="100"/>
							<TextBlock Grid.Column="0" Grid.Row="2" HorizontalAlignment="Left" Text="Высота стены, м:" Margin="0,5"/>
							<TextBox Grid.Column="1" Grid.Row="2" HorizontalAlignment="Left" x:Name="FirstWallHeight" Text="3" TextChanging="FirstWallHeight_TextChanged" Margin="0,5" Width="100"/>
							<TextBlock Grid.Column="2" Grid.Row="3" HorizontalAlignment="Left" Text="Величина звукоизоляции, Дб:" VerticalAlignment="Center" TextAlignment="Center" Width="110" TextWrapping="Wrap" Margin="15,45,0,0"/>
							<TextBlock Grid.Column="3" Grid.Row="3" HorizontalAlignment="Center" Text="Относительная площадь:" VerticalAlignment="Center" TextAlignment="Center" Width="110" TextWrapping="Wrap" Margin="-370,45,0,5"/>
							<TextBlock Grid.Column="3" Grid.Row="3" HorizontalAlignment="Left" Text="" VerticalAlignment="Center" TextAlignment="Center" Width="110" TextWrapping="Wrap" Margin="48,45,0,0"/>
							<TextBlock Grid.Column="0" Grid.Row="4" HorizontalAlignment="Left" Text="Материал стены:" Margin="0,5"/>
							<TextBox Grid.Column="2" Grid.Row="4" HorizontalAlignment="Left" x:Name="FirstWallMaterial_par1" Margin="20,0" Width="100" Height="20" />
							<TextBox Grid.Column="3" Grid.Row="4" HorizontalAlignment="Left" x:Name="FirstWallMaterial_par2" Margin="-100,5,0,5" Width="100" Height="20" />
							<TextBlock Grid.Column="4" Grid.Row="3" HorizontalAlignment="Left" Text=""  TextAlignment="Center" Width="110" Margin="-80,55,0,0" TextWrapping="Wrap" />
							<ComboBox Foreground="White"  Grid.Column="1"  Grid.Row="4" HorizontalAlignment="Left"  x:Name="comboBox_FirstWallMaterial" SelectionChanged="comboBox_FirstWallMaterial_Changed" Width="250" Margin="0,5" SelectedIndex="0">
								<ComboBoxItem Content="Ошибка"/>
							</ComboBox>
							<ComboBox Foreground="White"  Grid.Column="1"  Grid.Row="4" HorizontalAlignment="Left"  x:Name="comboBox_FirstWallMaterial_par" Width="250" Margin="0,5" SelectedIndex="0" IsVisible="False">
								<ComboBoxItem Content="Ошибка"/>
							</ComboBox>

							<TextBlock Grid.Column="0" Grid.Row="5" HorizontalAlignment="Left" Text="Смежная батаерея:" Margin="0,5"/>
							<TextBox Grid.Column="2" Grid.Row="5" HorizontalAlignment="Left" x:Name="FirstWallRadiator_par1" Margin="20,0" Width="100" Height="20" />
							<TextBox Grid.Column="3" Grid.Row="5" HorizontalAlignment="Left" x:Name="FirstWallRadiator_par2" Margin="-100,5,0,5" Width="100" Height="20" />
							<ComboBox Foreground="White"  Grid.Column="1"  Grid.Row="5" x:Name="comboBox_FirstWallRadiator" SelectionChanged="comboBox_FirstWallRadiator_Changed" Width="250" Margin="0,5" SelectedIndex="0">
								<ComboBoxItem Content="Ошибка"/>
							</ComboBox>
							<ComboBox Foreground="White"  Grid.Column="1"  Grid.Row="5" x:Name="comboBox_FirstWallRadiator_par" Width="250" Margin="0,5" SelectedIndex="0" IsVisible="False">
								<ComboBoxItem Content="Ошибка"/>
							</ComboBox>
							<TextBlock Grid.Column="0" Grid.Row="6" HorizontalAlignment="Left" Text="Окно:" Margin="0,5"/>
							<TextBox Grid.Column="2" Grid.Row="6" HorizontalAlignment="Left" x:Name="FirstWallWindow_par1" Margin="20,0" Width="100" Height="20" />
							<TextBox Grid.Column="3" Grid.Row="6" HorizontalAlignment="Left" x:Name="FirstWallWindow_par2" Margin="-100,5,0,5" Width="100" Height="20" />
							<ComboBox Foreground="White"  Grid.Column="1"  Grid.Row="6" x:Name="comboBox_FirstWallWindow" SelectionChanged="comboBox_FirstWallWindow_Changed" Width="250" Margin="0,5" SelectedIndex="0">
