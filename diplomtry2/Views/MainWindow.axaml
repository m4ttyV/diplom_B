							<TextBox Grid.Column="3" Grid.Row="7" HorizontalAlignment="Left" x:Name="FirstWallDoor_par2" Margin="-100,5,0,5" Height="20" Width="100" VerticalAlignment="Top"/>
							<ComboBox Foreground="White"  Grid.Column="1"  Grid.Row="7" x:Name="comboBox_FirstWallDoor" SelectionChanged="comboBox_FirstWallDoor_Changed" Width="250" Margin="0,5" SelectedIndex="0">
								<ComboBoxItem Content="Ошибка"/>
							</ComboBox>
							<ComboBox Foreground="White"  Grid.Column="1"  Grid.Row="5" x:Name="comboBox_FirstWallDoor_par" Width="250" Margin="0,5" SelectedIndex="0" IsVisible="False">
								<ComboBoxItem Content="Ошибка"/>
							</ComboBox>
						</Grid>
					</StackPanel>
				</TabItem>
