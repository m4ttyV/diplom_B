<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="https://github.com/avaloniaui/development"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="using:diplomtry2.ViewModels"
        xmlns:views="clr-namespace:diplomtry2.Views"
        mc:Ignorable="d"	
		MaxHeight="600"
		MaxWidth="1495"
		MinHeight="600"
		Background="#f0f0f0"
		MinWidth="1495"
        x:Class="diplomtry2.Views.MainWindow"
        Icon="avares://diplomtry2/Assets/logo.ico"
        Title="Лабораторная работа Виброакустика">
	<Window.Styles>
		<Style Selector="Button">
			<Setter Property="Background" Value="#6a6a6a"/>
			<Setter Property="Foreground" Value="White"/>			
		</Style>
		<Style Selector="TextBlock">
			<Setter Property="Foreground" Value="Black"></Setter>
		</Style>
		<Style Selector="ComboBox">
			<Setter Property="Foreground" Value="White"/>
		</Style>
		
	</Window.Styles>
	<Grid ColumnDefinitions="*">
		<StackPanel Orientation="Horizontal">
			<StackPanel Width="100">
				<Button x:Name="SelectFirstTab" Background="#4CAF50" Foreground="White" Margin="60,10,10,10" Height="40" Width="200" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" HorizontalAlignment="Center" Content="Стена 1" Click="TabButton1_Click"/>
				<Button x:Name="SelectSecondTab" Margin="60,10,10,10" Height="40" Width="200" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Стена 2" Click="TabButton2_Click"/>
				<Button x:Name="SelectThirdTab" Margin="60,10,10,10" Height="40" Width="200" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Стена 3" Click="TabButton3_Click"/>
				<Button x:Name="SelectFourthTab" Margin="60,10,10,10" Height="40" Width="200" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Стена 4" Click="TabButton4_Click"/>
				<Button x:Name="SelectFifthTab" Margin="60,10,10,10" Height="40" Width="200" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Расчёт стены 1" Click="TabButton5_Click" VerticalAlignment="Bottom"/>
				<Button x:Name="SelectSixthTab" Margin="60,10,10,10" Height="40" Width="200" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Расчёт стены 2" Click="TabButton6_Click" VerticalAlignment="Bottom"/>
				<Button x:Name="SelectSeventhTab" Margin="60,10,10,10" Height="40" Width="200" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Расчёт стены 3" Click="TabButton7_Click" VerticalAlignment="Bottom"/>
				<Button x:Name="SelectEightTab" Margin="60,10,10,10" Height="40" Width="200" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Расчёт стены 4" Click="TabButton8_Click" VerticalAlignment="Bottom"/>
				<Button x:Name="SelectNinthTab" Margin="60,10,10,10" Height="40" Width="200" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Общий расчёт" Click="TabButton9_Click" VerticalAlignment="Bottom"/>
				<Button x:Name="SelectHelpTab" Margin="60,10,10,10" Height="40" Width="200" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Content="Помощь" Click="TabButton10_Click" VerticalAlignment="Bottom"/>
			</StackPanel>
			<TabControl Name="tabControl" Width="1300" MaxWidth="1300" Margin="100,0,10,10">
