							<Image Grid.Column="4" Grid.Row="8" HorizontalAlignment="Left" VerticalAlignment="Bottom" x:Name="FirstWallImageODR" IsVisible="False" Source="avares://diplomtry2/Images/первая стена О-Д-Р.png" Width="200" Height="200" Margin="70,70,0,0"/>
							<Image Grid.Column="4" Grid.Row="8" HorizontalAlignment="Left" VerticalAlignment="Bottom" x:Name="FirstWallImageO" IsVisible="False" Source="avares://diplomtry2/Images/первая стена О----.png" Width="200" Height="200" Margin="70,70,0,0"/>
							<Image Grid.Column="4" Grid.Row="8" HorizontalAlignment="Left" VerticalAlignment="Bottom" x:Name="FirstWallImageD" IsVisible="False" Source="avares://diplomtry2/Images/первая стена --Д--.png" Width="200" Height="200" Margin="70,70,0,0"/>
							<Image Grid.Column="4" Grid.Row="8" HorizontalAlignment="Left" VerticalAlignment="Bottom" x:Name="FirstWallImageR" IsVisible="False" Source="avares://diplomtry2/Images/первая стена ----Р.png" Width="200" Height="200" Margin="70,70,0,0"/>
