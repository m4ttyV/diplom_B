							<Image Grid.Column="4" Grid.Row="8" HorizontalAlignment="Left" VerticalAlignment="Bottom" x:Name="SecondWallImageOR" IsVisible="False" Source="avares://diplomtry2/Images/вторая стена О---Р.png" Width="200" Height="200" Margin="70,70,0,00"/>
